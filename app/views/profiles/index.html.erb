<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    
      <tr>
        <td><%= @profiles.firstname %></td>
        <td><%= @profiles.lastname %></td>
        <td><%= @profiles.address %></td>
        <td><%= link_to 'Show', @profiles %></td>
        <td><%= link_to 'Edit', edit_profile_path(@profiles) %></td>
        <td><%= link_to 'Destroy', @profiles, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
